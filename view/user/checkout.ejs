        function selectAddress(element, addressId) {
            // Remove 'selected' class from all address elements
            document.querySelectorAll('.user-info').forEach(el => {
                el.classList.remove('selected');
            });
    
            // Add 'selected' class to the clicked address element
            element.classList.add('selected');
    
            // Update the selected address radio button
            document.querySelectorAll('input[name="selectedAddress"]').forEach(input => {
                if (input.value === addressId) {
                    input.checked = true;
                } else {
                    input.checked = false;
                }
            });
        }
    
